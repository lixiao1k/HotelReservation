<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="po">
    <class name="CreditPO" table="Credit">
        <id name="id" type="long">
            <column name="CREDIT_ID"></column>
            <generator class="native"></generator>
        </id>
        <property name="date" type="date">
            <column name="DATE"></column>
        </property>
        <property name="delta" type="int">
            <column name="DELTA"></column>
        </property>
        <property name="credit" type="int">
            <column name="CREDIT"></column>
        </property>
        <property name="reason" type="string">
            <column name="REASON"></column>
        </property>
        <many-to-one name="member" column="M_ID" class="MemberPO" not-null="true" lazy="proxy"></many-to-one>
    </class>
</hibernate-mapping>